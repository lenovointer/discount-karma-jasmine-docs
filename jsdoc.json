{
  "source": {
    "include": [
      "./src"
    ],
    "includePattern": "\\.js$"
  },
  "opts": {
    "template": "templates/docdash",
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}